<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean name="{{txtIdServiceName}}" class="org.egovframe.rte.fdl.idgnr.impl.EgovSequenceIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="{{txtDatasourceName}}"/>
        <property name="query"  value="{{txtQuery}}"/>
        {{#if (eq rdoIdType "BigDecimal")}}        
        <property name="useBigDecimals" value="true"/>
        {{/if}}        
    </bean>  
</beans>
